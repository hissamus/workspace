package arrays;

import java.util.regex.Pattern;

public class StringPattern {
	
	public static void main(String[] args) {
		
		/*
		 * 4
11
cAda
AbrAcadAbRa*/
		
		System.out.println(Pattern.matches("[cd[a]+]?", "AbrAcadAbRa"));
		System.out.println(Pattern.matches("[cada]?", "AbracadabRa"));
	}

}
