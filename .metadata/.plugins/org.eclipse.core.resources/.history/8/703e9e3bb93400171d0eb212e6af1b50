//        String L,L1,U;
//        for(int i=0;i<N.length();i++){
//        	U=null;
//        	L1=N.substring(i,i+1);
//        	L=N.substring(i,i+1).toLowerCase();        	
//        	if(L.equals(L1)){
//        		U=N.substring(i,i+1).toUpperCase();
//        		R.append(U);
//        	}else{
//        		R.append(L);
//        	}
//        }
//        
//        L=L1=U=null;
//        N=null;
//        System.out.println(R.toString());
//        }
}
}